@model MVCForum.Website.Areas.Admin.ViewModels.RecruitmentViewModel
@{
    ViewBag.Title = "Tạo mới Tuyển Dụng";
    ViewBag.ImageUploadType =  "forumimageinsert";
}


@Html.Partial("_RecruitmentNavButtons")

<div class="panel">
    <div class="panel-heading">
        <h5>@ViewBag.Title</h5>
    </div>
    <div class="panel-content">
        @using (Html.BeginForm("CreateRecruitment", "Common", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            @Html.ValidationSummary(false)
            @Html.AntiForgeryToken()

            @Html.Partial("_RecruitmentFields", Model)

            <div class="submit-holder">
                <input type="submit" class="btn-mvc-green btn-mvc-large" value="Create" />
            </div>
        }
    </div>
</div>